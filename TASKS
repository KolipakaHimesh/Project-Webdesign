#TAST -2
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dynamic Page</title>
  <style>
    /* Add CSS styles here */
  </style>
</head>
<body>
  <header>
    <h1>Welcome to Dynamic Page</h1>
  </header>
  <section id="dynamic-section" class="main-content">
    <!-- Dynamic content will be inserted here -->
  </section>

  <script>
    // Example of dynamic functionality using JavaScript
    window.addEventListener('DOMContentLoaded', function() {
      const dynamicSection = document.getElementById('dynamic-section');

      // Sample JSON data (replace with actual JSON objects)
      const jsonData = [
        { name: 'Item 1', description: 'Description for Item 1' },
        { name: 'Item 2', description: 'Description for Item 2' },
        { name: 'Item 3', description: 'Description for Item 3' }
      ];

      // Iterate through JSON data and create dynamic elements
      jsonData.forEach(item => {
        const div = document.createElement('div');
        div.innerHTML = `
          <h2>${item.name}</h2>
          <p>${item.description}</p>
        `;
        dynamicSection.appendChild(div);
      });
    });
  </script>
</body>
</html>
